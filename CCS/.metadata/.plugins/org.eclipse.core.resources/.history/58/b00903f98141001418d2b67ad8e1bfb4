#include <msp430.h> 

/*
 * main.c
 */
int main(void) {

	//высокие частоты
	int P1 = 27; //дл€ частоты 1209 √ц
	int P2 = 24; //дл€ частоты 1336 √ц
	int P3 = 22; //дл€ частоты 1477 √ц
	int P4 = 20; //дл€ частоты 1633 √ц

	/*дл€ того что бы был 50% рабочий цикл*/
	int D1 = 13;
	int D2 = 12;
	int D3 = 11;

	//низкие частоты
	int P5 = 47; //дл€ частоты 697 √ц
	int P6 = 42; //дл€ частоты 770 √ц
	int P7 = 38; //дл€ частоты 852 √ц
	int P8 = 34; //дл€ частоты 941 √ц

    WDTCTL = WDTPW | WDTHOLD;	// Stop watchdog timer
    FLL_CTL0 |= XCAP14PF;       // Configure load caps
	
    // Wait for xtal to stabilize
    do
    {
    	IFG1 &= ~OFIFG;      // Clear OSCFault flag
    	for (i = 0x47FF; i > 0; i--);   // Time for flag to set
    }
    while ((IFG1 & OFIFG)); // OSCFault flag still set?

    //конфигурируем пины
    P1DIR |= 0x04; // P1.2 output
    P1SEL |= 0x04; // P1.2 TA1 option


	return 0;
}
